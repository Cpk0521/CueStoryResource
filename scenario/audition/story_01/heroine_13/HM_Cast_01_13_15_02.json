    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "……えっ、その服。また転んだの？\n泥だらけだけど。"
            },
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "違いますよ―！\n今日は、泥団子を作って遊んでたんです！"
            },
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_02/hm_cast_01_13_15_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "そうなんだ。\n凛音は、本当に子どもたちが好きだね。"
            },
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "はい！\nみんな、すごく素直なんですよ。"
            },
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_02/hm_cast_01_13_15_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "笑った時の顔とか、かわいいんですよ。にこーって。\n見ていると、わたしも嬉しくなるんです！"
            },
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_02/hm_cast_01_13_15_02_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "今度、みんなでピクニックに行くんですけど。\nどこか、おすすめの場所ってありますか？"
            },
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_02/hm_cast_01_13_15_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "うーん、利恵なら最近ランニングも頑張っているらしいから。\nどこかいいスポットとか知っているかも。"
            },
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "そっか！　ありがとうございます！\n早速聞いて来ます。リエンヌー！"
            },
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_02/hm_cast_01_13_15_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "あ、走らないようにね……。"
            },
            "voice": ""
        }
    ]
}