        },
        {
            "index": 13,
            "heroineId": [
                16
            ],
            "name": {
                "default": "遠見鳴"
            },
            "message": {
                "default": "……生き返る。ありがとう。"
            },
            "voice": "card/Card_14/Card_4140008_2/card_4140008_2_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                14
            ],
            "name": {
                "default": "宇津木聡里"
            },
            "message": {
                "default": "どういたしまして。"
