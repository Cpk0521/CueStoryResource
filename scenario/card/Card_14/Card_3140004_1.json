        {
            "index": 26,
            "scenarioIndex": 65,
            "Type": 0,
            "heroineId": [
                14
            ],
            "name": {
                "default": "宇津木聡里"
            },
            "message": {
                "default": "……すぐに「ありがとう」って言わない方がいいよ。"
