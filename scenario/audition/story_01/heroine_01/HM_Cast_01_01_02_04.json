                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "あ、舞花ちゃん。どうしたの？"
            },
            "voice": "audition/story_01/heroine_01/HM_Cast_01_01_02_04/hm_cast_01_01_02_04_002.mp3"
