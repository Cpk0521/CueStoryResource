    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                102
            ],
            "name": {
                "default": "五十鈴りお"
            },
            "message": {
                "default": "ほんとだ……。\nバイクに乗った女の人がいる……。"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "すみません、お待たせしちゃって。"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "乗せてくれてありがとうございました。\nこれ、お礼です。受け取ってください。"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": {
                "default": "バイク乗りの女性"
            },
            "message": {
                "default": "そんなの受取れないわ。"
            },
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "受け取ってください。\n控室にあった飴ちゃんですけど。"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "飴ちゃんて……。"
            },
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": {
                "default": "バイク乗りの女性"
            },
            "message": {
                "default": "ふふっ。これならいただくわ。\nありがとう。"
            },
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "ホントでしたね……。"
            },
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                101
            ],
            "name": {
                "default": "鳳真咲"
            },
            "message": {
                "default": "そうね……。"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                102
            ],
            "name": {
                "default": "五十鈴りお"
            },
            "message": {
                "default": "見て、今度はすっごく大きな外車がやってきた……。"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "リムジンてやつですね……。"
            },
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                102
            ],
            "name": {
                "default": "五十鈴りお"
            },
            "message": {
                "default": "車からおばあちゃんが出て来た……。"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": {
                "default": "優しそうなお婆さん"
            },
            "message": {
                "default": "親切なお嬢さん、先ほどはどうも。"
            },
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "お友達のおうちを探してたおばあちゃん！"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "あの話も本当だったんだ……。"
            },
            "voice": ""
        },
        {
            "index": 16,
            "heroineId": [
                0
            ],
            "name": {
                "default": "優しそうなお婆さん"
            },
            "message": {
                "default": "もしかして、このスマホはあなたのかしら？"
            },
            "voice": ""
        },
        {
            "index": 17,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "あ、わたしのスマホ！\nどうして！？"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                0
            ],
            "name": {
                "default": "優しそうなお婆さん"
            },
            "message": {
                "default": "お友達の家の近くに落ちてたの。\nもしかしたらって思って。"
            },
            "voice": ""
        },
        {
            "index": 19,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "そっか！\n家の前でタクシーを呼んだとき落としたんだ！"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                102
            ],
            "name": {
                "default": "五十鈴りお"
            },
            "message": {
                "default": "タクシーってことは、もしかして……。"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "あ、もう1台、車が来ますよ？"
            },
            "voice": ""
        },
        {
            "index": 22,
            "heroineId": [
                102
            ],
            "name": {
                "default": "五十鈴りお"
            },
            "message": {
                "default": "今度は何……？"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                0
            ],
            "name": {
                "default": "？？？"
            },
            "message": {
                "default": "あなたが、凛音さん？\nあなたが助けてくれた、妊婦の姉です。"
            },
            "voice": ""
        },
        {
            "index": 24,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "あ、妊婦さんの！\nどうなりました？"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                0
            ],
            "name": {
                "default": "妊婦の姉"
            },
            "message": {
                "default": "無事、生まれました。\n元気な女の子です。"
            },
            "voice": ""
        },
        {
            "index": 26,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "よかった！\nおめでとうございます！"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                0
            ],
            "name": {
                "default": "妊婦の姉"
            },
            "message": {
                "default": "妹が、あなたのことをとても気に入っていてね。\n娘の名前を、あなたと同じ、凛音にしたいっていうの。"
            },
            "voice": ""
        },
        {
            "index": 28,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "タクシーの中で聞きました！\n嬉しいです！"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                0
            ],
            "name": {
                "default": "妊婦の姉"
            },
            "message": {
                "default": "妹も喜ぶわ。\nありがとう。"
            },
            "voice": ""
        },
        {
            "index": 30,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "社長、どうしましょう……。"
            },
            "voice": ""
        },
        {
            "index": 31,
            "heroineId": [
                101
            ],
            "name": {
                "default": "鳳真咲"
            },
            "message": {
                "default": "どうもこうもないわ。\n今回の遅刻は許すしかないでしょう？"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                102
            ],
            "name": {
                "default": "五十鈴りお"
            },
            "message": {
                "default": "ですよね……。"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                101
            ],
            "name": {
                "default": "鳳真咲"
            },
            "message": {
                "default": "凛音！\nそろそろライブの準備に入りなさい！"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "はーい！"
            },
            "voice": "card/Card_15/Card_4150005_3/card_4150005_3_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                0
            ],
            "name": {
                "default": "優しそうなお婆さん"
            },
            "message": {
                "default": "頑張って！\n客席から観させてもらうわ！"
            },
            "voice": ""
        },
        {
            "index": 36,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "ありがとうございます！\nよ～し！　今日も頑張っちゃうよー！"
