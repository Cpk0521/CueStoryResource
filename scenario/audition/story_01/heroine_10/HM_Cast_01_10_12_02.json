            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "いくつか頼んで、2人でシェアしたんです。\n1つ1つはそんなに大きくなかったので。"
            },
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_02/hm_cast_01_10_12_02_007.mp3"
