            "voice": "card/Card_01/Card_3010008_2/card_3010008_2_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "あんなこと、出来るんだね。"
            },
            "voice": "card/Card_01/Card_3010008_2/card_3010008_2_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                4
            ],
            "name": {
                "default": "月居ほのか"
            },
            "message": {
                "default": "ね。"
            },
            "voice": "card/Card_01/Card_3010008_2/card_3010008_2_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "あれってさ、ほのかちゃん。"
