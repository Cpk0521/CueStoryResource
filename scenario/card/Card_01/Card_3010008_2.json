            "voice": "card/Card_01/Card_3010008_2/card_3010008_2_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                4
            ],
            "name": {
                "default": "月居ほのか"
            },
            "message": {
                "default": "場違いなんてことは、ないんじゃない？"
