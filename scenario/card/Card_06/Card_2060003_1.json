        {
            "index": 5,
            "scenarioIndex": 21,
            "Type": 0,
            "heroineId": [
                103
            ],
            "name": {
                "default": "由良桐香"
            },
            "message": {
                "default": "それは……、正確にいうと、\n出来てることもあるし、出来てないこともある。"
