            "voice": "card/Card_01/Card_3010004_2/card_3010004_2_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "どういうことですか……？"
