            "voice": "main_01/Main_01_07/Main_01_07_09/main_01_07_09_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                4
            ],
            "name": {
                "default": "月居ほのか"
            },
            "message": {
                "default": "もともとライバル……、確かにそうだけど。"
