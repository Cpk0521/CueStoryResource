            "voice": "card/Card_09/Card_3090006_1/card_3090006_1_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                9
            ],
            "name": {
                "default": "夜峰美晴"
            },
            "message": {
                "default": "（結構スコアってあるんだなぁ……。）"
