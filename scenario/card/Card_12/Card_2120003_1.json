        {
            "index": 25,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "もーやめてって！　\nあんたなんかあたしに恨みでもあるの！"
            },
            "voice": "card/Card_12/Card_2120003_1/card_2120003_1_018.mp3"
        },
        {
            "index": 26,
