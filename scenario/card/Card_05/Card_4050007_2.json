            "voice": "card/Card_05/Card_4050007_2/card_4050007_2_027.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                5
            ],
            "name": {
                "default": "天童悠希"
            },
            "message": {
                "default": "料理が無事だし、問題なし！"
