            "voice": "card/Card_07/Card_3070013_1/card_3070013_1_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                6
            ],
            "name": {
                "default": "赤川千紗"
            },
            "message": {
                "default": "悠希ー！\n朝ご飯早く食べちゃってー！"
