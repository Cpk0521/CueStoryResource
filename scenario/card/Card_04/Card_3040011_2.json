        {
            "index": 2,
            "heroineId": [
                4
            ],
            "name": {
                "default": "月居ほのか"
            },
            "message": {
                "default": "（……ううん、自主トレはいつもしてるんだし、\nきっと大丈夫。）"
            },
            "voice": "card/Card_04/Card_3040011_2/card_3040011_2_002.mp3"
        },
        {
