    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "あ、マネージャーさん！\nお疲れ様です。"
            },
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_04/hm_main_01_07_04_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "お疲れ様、あいり。\n調子はどうかな。"
            },
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "えへへ、今日はとってもいい感じです！"
            },
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_04/hm_main_01_07_04_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "それは良かった。\nなにかいいことでもあったの？"
            },
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "はい！\n今日は悠希ちゃんたちと、お菓子パーティーをしたんです。"
            },
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_04/hm_main_01_07_04_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "柚葉ちゃんはお茶を淹れてくれて、\n千紗ちゃんはお菓子作ってくれて……。"
            },
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_04/hm_main_01_07_04_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "みんなでたくさんおしゃべりしました。\nふふっ、とっても楽しかったです。"
            },
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_04/hm_main_01_07_04_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "そうなんだ。\nいい息抜きになったみたいだね。"
            },
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "はい。\n悠希ちゃんたちは、きっと気付いてたんだと思います。"
            },
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_04/hm_main_01_07_04_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "あいり、ここ最近、ずっとそわそわしてたから……。"
            },
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_04/hm_main_01_07_04_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "そっかそっか……。"
            },
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "えへへ……あいりもそう思います。"
            },
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_04/hm_main_01_07_04_013.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "今度は、あいりがみんなの支えになりたいです。"
            },
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_04/hm_main_01_07_04_014.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "はい！\nあいりもたくさん頑張れそうです。"
            },
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_04/hm_main_01_07_04_016.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "よろしくお願いします、マネージャーさん！"
            },
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_04/hm_main_01_07_04_017.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "ええっ、そんなことないですよ！？"
            },
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_04/hm_main_01_07_04_019.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "マネージャーさんは、あいりに必要です！"
            },
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_04/hm_main_01_07_04_020.mp3"
