            "voice": "event/Event_004/Event_004_10/event_004_10_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                6
            ],
            "name": {
                "default": "赤川千紗"
            },
            "message": {
                "default": "『……あ。ピピティパピテぷぅ～！』"
