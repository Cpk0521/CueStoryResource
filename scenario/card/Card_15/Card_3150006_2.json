        {
            "index": 48,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "内緒なんて悪いですよ！\nこんなに美味しいのに！"
            },
            "voice": "card/Card_15/Card_3150006_2/card_3150006_2_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
