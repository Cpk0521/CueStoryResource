        },
        {
            "index": 20,
            "scenarioIndex": 53,
            "Type": 0,
            "heroineId": [
                14
            ],
            "name": {
                "default": "宇津木聡里"
            },
            "message": {
                "default": "だいたいそうだよ。\n2世っていうのは、舐められるの。"
