            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                103
            ],
            "name": {
                "default": "由良桐香"
            },
            "message": {
                "default": "昔の話よ。"
