            "voice": "card/Card_10/Card_2100003_2/card_2100003_2_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "えっ。"
