            "voice": ""
        },
        {
            "index": 23,
            "heroineId": [
                0
            ],
            "name": {
                "default": "鈴木プロデューサー"
            },
            "message": {
                "default": "制限時間は1時間。\nもちろん、いつおやめになっても結構。"
