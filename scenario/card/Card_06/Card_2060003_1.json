        {
            "index": 15,
            "scenarioIndex": 50,
            "Type": 0,
            "heroineId": [
                6
            ],
            "name": {
                "default": "赤川千紗"
            },
            "message": {
                "default": "いえ！ \nすると……、アイですか？　かなしみ？"
