        },
        {
            "index": 17,
            "heroineId": [
                5
            ],
            "name": {
                "default": "天童悠希"
            },
            "message": {
                "default": "そうだよ。"
            },
            "voice": "lesson/Lesson_20108/lesson_20108_016.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "じゃあ、わたしとあいりちゃんで悠希ちゃんの部屋から、\n制服とかカバンとか持って来てあげよう。"
            },
            "voice": "lesson/Lesson_20108/lesson_20108_017.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                5
            ],
            "name": {
                "default": "天童悠希"
            },
            "message": {
                "default": "え？　いいの？"
