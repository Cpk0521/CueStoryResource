            "voice": "card/Card_02/Card_3020004_1/card_3020004_1_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "観たことない？"
