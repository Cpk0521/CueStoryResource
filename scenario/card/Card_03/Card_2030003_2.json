    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                2
            ],
            "name": {
                "default": "鷹取舞花"
            },
            "message": {
                "default": "ほのか……、話しかけてみなよ……。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                4
            ],
            "name": {
                "default": "月居ほのか"
            },
            "message": {
                "default": "嫌だよ……、気持ち悪いもん……。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                2
            ],
            "name": {
                "default": "鷹取舞花"
            },
            "message": {
                "default": "じゃ、陽菜……。\nあんたリーダーでしょ……。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "わかった……、話しかけてみる……。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "志穂ちゃん、今日はいい天気だね。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                3
            ],
            "name": {
                "default": "鹿野志穂"
            },
            "message": {
                "default": "パピパピポーパパ。\n（確かにそうだな。）"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                2
            ],
            "name": {
                "default": "鷹取舞花"
            },
            "message": {
                "default": "マネージャー、これなんすか……？"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "分かんないよ、そんなこと……。"
            },
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                3
            ],
            "name": {
                "default": "鹿野志穂"
            },
            "message": {
                "default": "ピンパ、パパピポポポパ、ピピププパ。\n（みんな、私のことは気にするな。）"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "何て言ったの……？"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                4
            ],
            "name": {
                "default": "月居ほのか"
            },
            "message": {
                "default": "わかるわけないでしょ……。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                3
            ],
            "name": {
                "default": "鹿野志穂"
            },
            "message": {
                "default": "パッパピ、プパパパパピパ……。\n（やっぱり、伝わらないか……。）"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                2
            ],
            "name": {
                "default": "鷹取舞花"
            },
            "message": {
                "default": "もう限界……、ねぇ、志穂。\n気持ち悪いからそれ、やめてくれない？"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                3
            ],
            "name": {
                "default": "鹿野志穂"
            },
            "message": {
                "default": "ポぺパ、ぺピパピ。\n（それは出来ない。）"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                2
            ],
            "name": {
                "default": "鷹取舞花"
            },
            "message": {
                "default": "もういいよ。\n志穂とは話したくない。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "舞花ちゃん、どこ行くの？　\nクレープ屋さんは？"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                2
            ],
            "name": {
                "default": "鷹取舞花"
            },
            "message": {
                "default": "志穂と一緒に行きたくない。\n先に帰る。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                4
            ],
            "name": {
                "default": "月居ほのか"
            },
            "message": {
                "default": "あーあ、怒っちゃった……。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                2
            ],
            "name": {
                "default": "鷹取舞花"
            },
            "message": {
                "default": "じゃあね！"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "あれ……？　\n舞花ちゃん、信号赤なのに渡ろうとしてる……？"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "気付いてないんだ……、舞花！"
            },
            "voice": ""
        },
        {
            "index": 22,
            "heroineId": [
                3
            ],
            "name": {
                "default": "鹿野志穂"
            },
            "message": {
                "default": "パプパピ！\n（危ない！）"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                2
            ],
            "name": {
                "default": "鷹取舞花"
            },
            "message": {
                "default": "えっ？\n危ない？"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "危なかった……。"
            },
            "voice": ""
        },
        {
            "index": 25,
            "heroineId": [
                2
            ],
            "name": {
                "default": "鷹取舞花"
            },
            "message": {
                "default": "今、志穂の言ってることがわかった……。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                3
            ],
            "name": {
                "default": "鹿野志穂"
            },
            "message": {
                "default": "思わずパピプぺポで叫んじゃった……。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                2
            ],
            "name": {
                "default": "鷹取舞花"
            },
            "message": {
                "default": "ねっ、このヨーグルト志穂の？"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                3
            ],
            "name": {
                "default": "鹿野志穂"
            },
            "message": {
                "default": "ポゥ。ポピパッパパパプ。\n（そう。欲しかったらやる。）"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                2
            ],
            "name": {
                "default": "鷹取舞花"
            },
            "message": {
                "default": "やったー！\nサンキュー。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                3
            ],
            "name": {
                "default": "鹿野志穂"
            },
            "message": {
                "default": "ポポパパピ、ポンポパパパピーポ、パッペプぺ。\n（その代わり、今度新しいの買ってくれ。）"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                2
            ],
            "name": {
                "default": "鷹取舞花"
            },
            "message": {
                "default": "わかった。\n約束する。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                3
            ],
            "name": {
                "default": "鹿野志穂"
            },
            "message": {
                "default": "ペッパピパポ。\n（絶対だぞ。）"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                4
            ],
            "name": {
                "default": "月居ほのか"
            },
            "message": {
                "default": "いつの間にか、普通に会話してる……！"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "わたしも、志穂ちゃんの言葉がわかるようになってきた……。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                4
            ],
            "name": {
                "default": "月居ほのか"
            },
            "message": {
                "default": "実はわたしも……。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                103
            ],
            "name": {
                "default": "由良桐香"
            },
            "message": {
                "default": "はい。今日のレッスンはここまで。\n鹿野さん、見違えるほど心で芝居ができるようになったわね。"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                3
            ],
            "name": {
                "default": "鹿野志穂"
            },
            "message": {
                "default": "パピ、ポップンポ、ポパペペプ。\n（はい、特訓のおかげです。）"
            },
            "voice": "card/Card_03/Card_2030003_2/card_2030003_2_000.mp3"
        },
