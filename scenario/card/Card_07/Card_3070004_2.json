            "voice": "card/Card_07/Card_3070004_2/card_3070004_2_057.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "どこにいってたの？　\n野宿したの？　"
