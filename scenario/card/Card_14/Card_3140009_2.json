            "voice": "card/Card_14/Card_3140009_2/card_3140009_2_008.mp3"
        },
        {
            "index": 9,
            "scenarioIndex": 28,
            "Type": 0,
            "heroineId": [
                0
            ],
            "name": {
                "default": "店長"
            },
            "message": {
                "default": "ありがとう。助かるわ。"
