            "voice": "main_01/Main_01_07/Main_01_07_09/main_01_07_09_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                14
            ],
            "name": {
                "default": "宇津木聡里"
            },
            "message": {
                "default": "そう？　不安しかないんだけど。"
