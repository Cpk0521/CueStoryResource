            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "えっ？"
            },
            "voice": ""
        },
        {
            "index": 53,
            "heroineId": [
                8
            ],
            "name": {
                "default": "九条柚葉"
            },
            "message": {
                "default": "入寮の説明って？"
            },
            "voice": "main_01/Main_01_02/Main_01_02_03/main_01_02_03_053.mp3"
        },
        {
            "index": 54,
            "heroineId": [
