        },
        {
            "index": 56,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "それはそうだねー。\nじゃあ、今日はもう寝ちゃうね！"
            },
            "voice": "event/Event_006/Event_006_10/event_006_10_056.mp3"
        },
