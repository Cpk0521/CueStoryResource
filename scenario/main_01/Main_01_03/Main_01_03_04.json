        },
        {
            "index": 17,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "知らない。\nすぐシャワー浴びて寝たから。"
            },
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "……。"
            },
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "……。"
