            "voice": ""
        },
        {
            "index": 59,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "ずっとツイてたんだけどなぁ……。"
            },
            "voice": "card/Card_12/Card_3120004_2/card_3120004_2_059.mp3"
        },
        {
            "index": 60,
            "heroineId": [
