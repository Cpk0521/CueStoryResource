            "voice": "card/Card_10/Card_2100003_2/card_2100003_2_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "絢ぁ……。"
            },
            "voice": "card/Card_10/Card_2100003_2/card_2100003_2_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "莉子……、私……。"
