            "voice": "main_01/Main_01_05/Main_01_05_07/main_01_05_07_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "この朗読劇は遊びじゃないの。"
