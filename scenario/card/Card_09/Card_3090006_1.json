            "voice": "card/Card_09/Card_3090006_1/card_3090006_1_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "最近美晴、部屋に閉じこもってばっかりだね。"
