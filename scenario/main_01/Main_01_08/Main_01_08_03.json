            ],
            "name": {
                "default": "鹿野志穂"
            },
            "message": {
                "default": "ほう。これはなかなか……。"
            },
            "voice": "main_01/Main_01_08/Main_01_08_03/main_01_08_03_053.mp3"
        },
        {
