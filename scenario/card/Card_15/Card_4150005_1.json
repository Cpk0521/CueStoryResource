        },
        {
            "index": 5,
            "heroineId": [
                102
            ],
            "name": {
                "default": "五十鈴りお"
            },
            "message": {
                "default": "凛音ちゃんが遅刻なんて珍しいね……。"
            },
            "voice": "card/Card_15/Card_4150005_1/card_4150005_1_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                101
            ],
            "name": {
                "default": "鳳真咲"
            },
            "message": {
                "default": "どう？\nいた？"
