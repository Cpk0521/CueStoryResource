            "voice": "card/Card_03/Card_3030006_2/card_3030006_2_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "2時間も動かないでいるのは無理だよ！"
