            "voice": "card/Card_01/Card_3010008_2/card_3010008_2_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "うん……。だから、わたし……。"
