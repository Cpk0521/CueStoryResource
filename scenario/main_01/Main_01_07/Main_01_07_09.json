            },
            "voice": "main_01/Main_01_07/Main_01_07_09/main_01_07_09_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                4
            ],
            "name": {
                "default": "月居ほのか"
            },
            "message": {
                "default": "え？\n……うん。わかった。"
