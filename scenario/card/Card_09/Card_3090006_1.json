            "voice": "card/Card_09/Card_3090006_1/card_3090006_1_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "私も。"
