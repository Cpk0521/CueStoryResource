            "voice": "main_01/Main_01_08/Main_01_08_03/main_01_08_03_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "はい……。"
            },
            "voice": "main_01/Main_01_08/Main_01_08_03/main_01_08_03_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                102
            ],
            "name": {
                "default": "五十鈴りお"
            },
            "message": {
                "default": "というわけで、取材日が決まったらまた連絡するね。"
