        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "おやすみなさい。"
            },
            "voice": "card/Card_07/Card_4070011_3/card_4070011_3_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                5
            ],
            "name": {
                "default": "天童悠希"
            },
            "message": {
                "default": "あ、うん、おやすみー。"
            },
            "voice": "card/Card_07/Card_4070011_3/card_4070011_3_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": {
                "default": "天童悠希"
            },
            "message": {
                "default": "……ねぇ、今のあいり、ちょっと変じゃなかった？"
            },
            "voice": "card/Card_07/Card_4070011_3/card_4070011_3_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                8
            ],
            "name": {
                "default": "九条柚葉"
            },
            "message": {
                "default": "アイリ……。"
