            "voice": "card/Card_07/Card_3070013_1/card_3070013_1_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                5
            ],
            "name": {
                "default": "天童悠希"
            },
            "message": {
                "default": "あ、そっか！\nありがと千紗！"
