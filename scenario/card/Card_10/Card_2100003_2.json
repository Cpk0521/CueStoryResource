        },
        {
            "index": 38,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "あなためちゃめちゃおもしろかった。"
            },
            "voice": "card/Card_10/Card_2100003_2/card_2100003_2_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "っへ？"
            },
