            "voice": "card/Card_07/Card_3070004_2/card_3070004_2_059.mp3"
        },
        {
            "index": 59,
            "heroineId": [
                6
            ],
            "name": {
                "default": "赤川千紗"
            },
            "message": {
                "default": "どういうことよ！　\nもっと詳しく説明しなさい！"
