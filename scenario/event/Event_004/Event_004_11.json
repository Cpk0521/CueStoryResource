            "voice": "event/Event_004/Event_004_11/event_004_11_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "……そうだね！"
