        {
            "index": 47,
            "scenarioIndex": 163,
            "Type": 0,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "……いや、多分、昔のまほろには見えなかったものが、\n今は、見えるようになっただけなのかも。"
            },
            "voice": "card/Card_11/Card_4110009_3/card_4110009_3_047.mp3"
        },
        {
            "index": 48,
