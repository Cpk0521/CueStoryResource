            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                13
            ],
            "name": {
                "default": "丸山利恵"
            },
            "message": {
                "default": "悪いが、ジャージを選ぶ。"
