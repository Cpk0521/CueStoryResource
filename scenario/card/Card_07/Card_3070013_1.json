        },
        {
            "index": 26,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "悠希ちゃん！？　寝ちゃダメだよお……。\n本当に遅刻しちゃうよ！？"
            },
            "voice": "card/Card_07/Card_3070013_1/card_3070013_1_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                5
            ],
            "name": {
                "default": "天童悠希"
            },
            "message": {
                "default": "うん？　……うーん。"
