        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "そこで、笑いに自信のある5人に集まってもらいました。"
            },
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                2
            ],
            "name": {
                "default": "鷹取舞花"
            },
            "message": {
                "default": "どうも！\nバラエティ担当の舞花です！"
