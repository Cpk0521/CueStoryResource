        },
        {
            "index": 20,
            "heroineId": [
                103
            ],
            "name": {
                "default": "由良桐香"
            },
            "message": {
                "default": "なるほど驚いた。\n……よくやったわね。"
            },
            "voice": "card/Card_10/Card_2100003_2/card_2100003_2_021.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "……っえ？"
