            "voice": "card/Card_01/Card_4010010_2/card_4010010_2_036.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "そう……。　"
