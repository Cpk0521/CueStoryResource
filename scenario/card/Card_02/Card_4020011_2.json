            "voice": "card/Card_02/Card_4020011_2/card_4020011_2_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                2
            ],
            "name": {
                "default": "鷹取舞花"
            },
            "message": {
                "default": "これは？"
