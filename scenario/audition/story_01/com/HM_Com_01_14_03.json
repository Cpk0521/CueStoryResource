                13
            ],
            "name": {
                "default": "丸山利恵"
            },
            "message": {
                "default": "あ、そうだ聡里、借りてた本返すね。\nありがとう。"
            },
            "voice": "audition/story_01/com/HM_Com_01_14_03/hm_com_01_14_03_001.mp3"
