                6
            ],
            "name": {
                "default": "赤川千紗"
            },
            "message": {
                "default": "作品と、自分に向き合って。\n……あとは実力を出すだけ。"
            },
            "voice": "audition/story_01/heroine_06/HM_Main_01_06_05/hm_main_01_06_05_006.mp3"
