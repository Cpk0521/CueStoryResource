            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "（2人がとんでもない方向に脱線しちゃってる。\n　これはいよいよ止めないと……。）"
            },
            "voice": "lesson/Lesson_20212/lesson_20212_003.mp3"
