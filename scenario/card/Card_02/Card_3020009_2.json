            "voice": "card/Card_02/Card_3020009_2/card_3020009_2_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                2
            ],
            "name": {
                "default": "鷹取舞花"
            },
            "message": {
                "default": "いや、そうだけどさ、ゲームだしこれ……。"
