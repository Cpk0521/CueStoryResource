            "voice": "card/Card_04/Card_4040013_3/card_4040013_3_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                4
            ],
            "name": {
                "default": "月居ほのか"
            },
            "message": {
                "default": "ん？　何が？"
