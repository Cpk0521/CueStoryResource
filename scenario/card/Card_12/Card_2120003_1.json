                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "慰めてくれたのね、ありがとう。\nん？　立ち上がった！？　さあ動いて！　力の限り！　動いて！"
            },
            "voice": "card/Card_12/Card_2120003_1/card_2120003_1_020.mp3"
