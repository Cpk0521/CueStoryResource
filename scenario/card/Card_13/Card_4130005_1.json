            "voice": "card/Card_13/Card_4130005_1/card_4130005_1_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                6
            ],
            "name": {
                "default": "赤川千紗"
            },
            "message": {
                "default": "そんなこと出来るわけないでしょ。"
