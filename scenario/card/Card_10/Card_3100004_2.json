        },
        {
            "index": 19,
            "heroineId": [
                9
            ],
            "name": {
                "default": "夜峰美晴"
            },
            "message": {
                "default": "笑わないよ。"
            },
            "voice": "card/Card_10/Card_3100004_2/card_3100004_2_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "ペットショップの犬が欲しいの。"
