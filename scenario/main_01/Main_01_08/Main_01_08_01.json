        },
        {
            "index": 26,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "それに志穂さんも。"
            },
            "voice": "main_01/Main_01_08/Main_01_08_01/main_01_08_01_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                3
            ],
            "name": {
                "default": "鹿野志穂"
            },
            "message": {
                "default": "こんなところで立ち話か？"
            },
            "voice": "main_01/Main_01_08/Main_01_08_01/main_01_08_01_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "うん。\r\nなんだか冬だねって。"
