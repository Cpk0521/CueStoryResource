        },
        {
            "index": 21,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "なるほど。\nそうですね。そうしましょう！"
            },
            "voice": "lesson/Lesson_20108/lesson_20108_020.mp3"
