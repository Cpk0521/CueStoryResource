            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "……ごめん、言い過ぎた。\nでも、あたしの気持ちだってわかってよね。"
            },
            "voice": "card/Card_12/Card_2120003_1/card_2120003_1_019.mp3"
