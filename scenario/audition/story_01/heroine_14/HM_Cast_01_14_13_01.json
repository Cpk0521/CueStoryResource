            "index": 5,
            "heroineId": [
                13
            ],
            "name": {
                "default": "丸山利恵"
            },
            "message": {
                "default": "本屋でしょ？\n我輩も買うものあるから、一緒に行こう？"
            },
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_13_01/hm_cast_01_14_13_01_005.mp3"
