            "voice": "card/Card_10/Card_3100004_2/card_3100004_2_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "……全然笑える要素なかったよ。"
