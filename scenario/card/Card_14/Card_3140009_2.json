            "voice": "card/Card_14/Card_3140009_2/card_3140009_2_021.mp3"
        },
        {
            "index": 22,
            "scenarioIndex": 74,
            "Type": 0,
            "heroineId": [
                0
            ],
            "name": {
                "default": "店長"
            },
            "message": {
                "default": "そうね。仕方ないんだけどね。"
