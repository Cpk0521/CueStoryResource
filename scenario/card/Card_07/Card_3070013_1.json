            "voice": "card/Card_07/Card_3070013_1/card_3070013_1_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "あっ、悠希ちゃん、おはよー……。"
