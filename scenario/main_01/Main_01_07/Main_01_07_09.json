            "voice": "main_01/Main_01_07/Main_01_07_09/main_01_07_09_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                4
            ],
            "name": {
                "default": "月居ほのか"
            },
            "message": {
                "default": "うん。そうだよ。\nわたしは……、誰かのヒーローに……。"
