        },
        {
            "index": 4,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "ほら、これ。\nこの雑貨屋さん、絶対に絢の好みだと思って。"
            },
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_11_02/hm_cast_01_10_11_02_004.mp3"
