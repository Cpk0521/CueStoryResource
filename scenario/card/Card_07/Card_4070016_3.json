        },
        {
            "index": 16,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "息が真っ白。\nそうだよね、雪が降ってるんだもん。"
            },
            "voice": "card/Card_07/Card_4070016_3/card_4070016_3_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "……。"
