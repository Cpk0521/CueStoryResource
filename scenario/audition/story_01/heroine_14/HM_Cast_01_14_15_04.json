    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "お疲れ様でーす！"
            },
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_04/hm_cast_01_14_15_04_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                14
            ],
            "name": {
                "default": "宇津木聡里"
            },
            "message": {
                "default": "お疲れ様です。\nさ、凛音帰るわよ。"
            },
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_04/hm_cast_01_14_15_04_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "あ、さとちん！\n帰りにさとちんのお花屋さん寄っていい？"
            },
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_04/hm_cast_01_14_15_04_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                14
            ],
            "name": {
                "default": "宇津木聡里"
            },
            "message": {
                "default": "別にいいけど、どうしたの？"
            },
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_04/hm_cast_01_14_15_04_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "えへへ、前にもらったラベンダーのポプリ、\n子どもたちにあげたいんだー。"
            },
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_04/hm_cast_01_14_15_04_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "ラベンダーって、こんなにいい匂いするんだよーって！"
            },
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_04/hm_cast_01_14_15_04_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "本当はお花であげたいんだけど、\n枯れちゃったらもったいないしねー。"
            },
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_04/hm_cast_01_14_15_04_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                14
            ],
            "name": {
                "default": "宇津木聡里"
            },
            "message": {
                "default": "そう。\n確か、まだ残ってるはずよ。"
            },
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_04/hm_cast_01_14_15_04_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "やったー！　じゃあ、早く行こうよー！\nお先失礼しまーす！"
            },
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_04/hm_cast_01_14_15_04_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                14
            ],
            "name": {
                "default": "宇津木聡里"
            },
            "message": {
                "default": "あ、ちょっと、凛音！\nまったく……。"
            },
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_04/hm_cast_01_14_15_04_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                14
            ],
            "name": {
                "default": "宇津木聡里"
            },
            "message": {
                "default": "じゃあ、私も失礼します。"
            },
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_04/hm_cast_01_14_15_04_011.mp3"
        }
    ]
}