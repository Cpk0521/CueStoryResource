            "voice": "main_01/Main_01_05/Main_01_05_09/main_01_05_09_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "みんな……。\nありがとう……。"
            },
            "voice": "main_01/Main_01_05/Main_01_05_09/main_01_05_09_056.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "あいり……。"
            },
            "voice": "main_01/Main_01_05/Main_01_05_09/main_01_05_09_057.mp3"
        },
        {
            "index": 58,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
