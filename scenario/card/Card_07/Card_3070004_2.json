            "voice": ""
        },
        {
            "index": 58,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "田舎街の……、おばあさんの家の……。\n高級な布団で寝てました。"
