        },
        {
            "index": 15,
            "heroineId": [
                9
            ],
            "name": {
                "default": "夜峰美晴"
            },
            "message": {
                "default": "どうしてそんなこと聞くの？"
            },
            "voice": "card/Card_10/Card_3100004_2/card_3100004_2_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "……。"
