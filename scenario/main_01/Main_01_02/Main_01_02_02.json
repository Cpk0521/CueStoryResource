            "voice": "main_01/Main_01_02/Main_01_02_02/main_01_02_02_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                101
            ],
            "name": {
                "default": "鳳真咲"
            },
            "message": {
                "default": "どうぞ。"
            },
            "voice": "main_01/Main_01_02/Main_01_02_02/main_01_02_02_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "すぅー……。\nはぁー……。"
