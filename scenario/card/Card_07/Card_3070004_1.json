            "voice": "card/Card_07/Card_3070004_1/card_3070004_1_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "（これでいいんだ。\nなーんにもないところで、あいり１人の力で、一晩過ごしてみせるんだ。）"
