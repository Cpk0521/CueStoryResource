            "voice": "card/Card_07/Card_3070004_2/card_3070004_2_056.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "そうか……。\nごめんなさい。"
