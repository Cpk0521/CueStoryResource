            "voice": "card/Card_07/Card_3070004_1/card_3070004_1_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                6
            ],
            "name": {
                "default": "赤川千紗"
            },
            "message": {
                "default": "……あいりが意見を言うなんて珍しいね。"
