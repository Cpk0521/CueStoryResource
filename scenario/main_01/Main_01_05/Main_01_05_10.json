            "voice": "main_01/Main_01_05/Main_01_05_10/main_01_05_10_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                3
            ],
            "name": {
                "default": "鹿野志穂"
            },
            "message": {
                "default": "いつまで教えてもらうつもりだ？"
            },
            "voice": "main_01/Main_01_05/Main_01_05_10/main_01_05_10_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "志穂ちゃん……、いつまでって……。"
