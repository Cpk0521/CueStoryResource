        },
        {
            "index": 2,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "ちょっと、しっ、しっ！"
            },
            "voice": "main_01/Main_01_03/Main_01_03_08/main_01_03_08_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "なんかあやしい。"
