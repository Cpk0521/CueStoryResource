            "voice": "card/Card_01/Card_3010008_2/card_3010008_2_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "ほら、この街と同じ。"
