            "voice": "card/Card_14/Card_4140008_2/card_4140008_2_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                16
            ],
            "name": {
                "default": "遠見鳴"
            },
            "message": {
                "default": "……いる。"
