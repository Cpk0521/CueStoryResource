            "voice": "card/Card_03/Card_3030006_2/card_3030006_2_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                3
            ],
            "name": {
                "default": "鹿野志穂"
            },
            "message": {
                "default": "とりあえず、2時間くらい。"
            },
            "voice": "card/Card_03/Card_3030006_2/card_3030006_2_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "2時間！？"
