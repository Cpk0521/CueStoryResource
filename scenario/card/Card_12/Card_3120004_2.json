        },
        {
            "index": 51,
            "heroineId": [
                0
            ],
            "name": {
                "default": "スーツの女性"
            },
            "message": {
                "default": "……さん、おめでとうございます。"
            },
            "voice": ""
        },
        {
            "index": 52,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "え……。"
            },
            "voice": ""
        },
        {
            "index": 53,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "ごめんね、力不足で。"
