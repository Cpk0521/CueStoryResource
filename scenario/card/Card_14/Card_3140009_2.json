            "voice": "card/Card_14/Card_3140009_2/card_3140009_2_005.mp3"
        },
        {
            "index": 6,
            "scenarioIndex": 23,
            "Type": 0,
            "heroineId": [
                0
            ],
            "name": {
                "default": "店長"
            },
            "message": {
                "default": "ふふ。本当に勉強家だね。"
