            "voice": "card/Card_02/Card_4020011_2/card_4020011_2_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                4
            ],
            "name": {
                "default": "月居ほのか"
            },
            "message": {
                "default": "いやあ、さすがにそろそろコロッケはちょっと……。"
