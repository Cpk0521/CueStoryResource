            ],
            "name": {
                "default": "九条柚葉"
            },
            "message": {
                "default": "え……。あれ？"
            },
            "voice": "card/Card_08/Card_4080009_2/card_4080009_2_056.mp3"
        },
        {
            "index": 57,
            "heroineId": [
