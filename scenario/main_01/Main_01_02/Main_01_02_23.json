            "voice": "main_01/Main_01_02/Main_01_02_23/main_01_02_23_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                6
            ],
            "name": {
                "default": "赤川千紗"
            },
            "message": {
                "default": "やったことないんですけど……。"
