        },
        {
            "index": 57,
            "heroineId": [
                4
            ],
            "name": {
                "default": "月居ほのか"
            },
            "message": {
                "default": "うん。いい人で良かった。"
            },
            "voice": "card/Card_04/Card_4040013_3/card_4040013_3_057.mp3"
        },
        {
            "index": 58,
            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "ほのかちゃんのお友達がオススメするお店だもん、\nいいお店に決まってるよ。"
