            "voice": "card/Card_07/Card_3070013_1/card_3070013_1_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "えっ！？\nどうしたの、悠希ちゃん？"
