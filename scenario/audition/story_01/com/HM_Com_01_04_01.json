        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": {
                "default": "月居ほのか"
            },
            "message": {
                "default": "はい！\n毎日ちゃんと体調管理してますから！"
            },
            "voice": "audition/story_01/com/HM_Com_01_04_01/hm_com_01_04_01_003.mp3"
