        },
        {
            "index": 24,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "ええっ、そんな時間あるかなぁ……。"
            },
            "voice": "card/Card_07/Card_3070013_1/card_3070013_1_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                5
            ],
            "name": {
                "default": "天童悠希"
            },
            "message": {
                "default": "……くぅ。"
