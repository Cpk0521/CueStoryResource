        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "ゆ、柚葉ちゃん……！\nマネージャーさんにまで言わなくても。"
            },
            "voice": "audition/story_01/heroine_07/HM_Cast_01_07_08_04/hm_cast_01_07_08_04_002.mp3"
