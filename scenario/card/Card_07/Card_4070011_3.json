            "voice": "card/Card_07/Card_4070011_3/card_4070011_3_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                6
            ],
            "name": {
                "default": "赤川千紗"
            },
            "message": {
                "default": "……あー。そういうこと、か……。"
