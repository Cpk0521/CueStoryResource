    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "え？　乗車券がないって……。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "改札通ったんでしょ？\nだったら無いはずないじゃん？"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "うん……。\nそれはそうなんだけど。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "財布とかカバンの中とか、全部探した？"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "うん。探したんだけど、見つからなくて……。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                9
            ],
            "name": {
                "default": "夜峰美晴"
            },
            "message": {
                "default": "この流れだし、うん、きっと5のEよ。\nこのまま座ってればいいんじゃない？"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "そういう話じゃないでしょ。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": {
                "default": "夜峰美晴"
            },
            "message": {
                "default": "えー、ダメかな？"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "車掌さん、確認に来るからね。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": {
                "default": "夜峰美晴"
            },
            "message": {
                "default": "そうなの？　それじゃ無賃乗車だと思われちゃう。\nえ！　やだ！　逮捕されちゃう？"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "いきなりそこまでのことはないと思うけどさ。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "どうしよう……。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "……。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "ジャケットの内ポケット……、探した？"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "え？　ちょっとまって……。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "あ、本当だ。あった！"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "どうしてわかったの？"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "そんな驚くこと？　\n簡単でしょ。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "改札通った後の乗車券、カバンや財布にしまってないなら、\n後はポケットしかないし。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "急いで着替え取りに行ってバタバタして、\nちょっと暑くなったからって、"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "絢、ホームに来るまで、\nコートは手に掛けてたでしょ？"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "あ、そうだった。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "コートを着てないなら、\n行き先は必然的にジャケットになるよね。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "そして……。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "人間の心理的に、なくすとまずいような重要な物は、\n外側じゃなく内側のポケットに入れがちなのよ。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "はー！　なーるほどねぇ……！"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "そして、上からコートを着ちゃうと、\nジャケットまで気が回らなくなる。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                9
            ],
            "name": {
                "default": "夜峰美晴"
            },
            "message": {
                "default": "まほろ、すごーい！\nまるで名探偵みたい！"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "いいって、そういうの……。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "……で、席番号は？\n7号車の5のE、で合ってる？"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "……あれ？　全然違う。\n6号車の12のCだって。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "は？"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                9
            ],
            "name": {
                "default": "夜峰美晴"
            },
            "message": {
                "default": "あら、どうして？"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "私だけ別の車両みたい……。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "多分マネージャーの手違いじゃない？"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                9
            ],
            "name": {
                "default": "夜峰美晴"
            },
            "message": {
                "default": "ええー。4人、一緒がいいのにー。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "……絢。\nその乗車券……、まほろのと交換しよ。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "え？"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "まほろ、ちょうど1人になりたかったし。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "だけど……。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                9
            ],
            "name": {
                "default": "夜峰美晴"
            },
            "message": {
                "default": "えー。わたしが行くよ？　\nみんなはここにいて。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "ほら、絢。早く渡して。\n結局、誰かはその席に行かなきゃなんないんだからさ。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "いいの？　まほろ？"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                9
            ],
            "name": {
                "default": "夜峰美晴"
            },
            "message": {
                "default": "わたしが行くのにー。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "まほろは、基本的に1人のほうが好きなの。\n知ってるでしょ？"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "……そっか。わかった。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "絢、早くまほろに渡してあげな。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "……うん。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "じゃあ、そういうことで。\n青森駅のホームで待ち合わせね。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "なんだかまほろに悪いことしちゃった……。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "まあ、1人になりたいっての、何割かは本音だから。\n気にしなくていいと思うよ。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "そうかもしれないけど。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                9
            ],
            "name": {
                "default": "夜峰美晴"
            },
            "message": {
                "default": "……。"
            },
            "voice": "event/Event_005/Event_005_02/event_005_02_053.mp3"
        }
    ]
