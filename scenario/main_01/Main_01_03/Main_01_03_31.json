            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "いや、それでよかったんだ。\nたぶんね……。"
