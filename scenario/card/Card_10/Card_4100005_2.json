            "voice": "card/Card_10/Card_4100005_2/card_4100005_2_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "皆、よろしくね。"
