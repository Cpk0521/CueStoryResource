            "heroineId": [
                1
            ],
            "name": {
                "default": "六石陽菜"
            },
            "message": {
                "default": "……実は、今朝もほのかちゃんと話をしてて。"
            },
            "voice": "audition/story_01/heroine_04/HM_Cast_01_04_01_03/hm_cast_01_04_01_03_004.mp3"
