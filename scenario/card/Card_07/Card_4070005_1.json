            "voice": "card/Card_07/Card_4070005_1/card_4070005_1_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "（さて、どうやって励まそうかな……。）"
            },
            "voice": ""
        },
        {
            "index": 23,
            "heroineId": [
                7
            ],
            "name": {
                "default": "恵庭あいり"
            },
            "message": {
                "default": "なのに……。\n変なんです……。"
