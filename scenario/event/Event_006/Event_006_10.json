            "voice": "event/Event_006/Event_006_10/event_006_10_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                15
            ],
            "name": {
                "default": "明神凛音"
            },
            "message": {
                "default": "そっかー……。"
