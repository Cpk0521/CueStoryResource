            "voice": "card/Card_09/Card_3090006_1/card_3090006_1_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "あ、みんなそんな感じなんだ……。"
