            "voice": "card/Card_09/Card_3090006_1/card_3090006_1_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "カルテットじゃないってこと。"
            },
            "voice": "card/Card_09/Card_3090006_1/card_3090006_1_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "ああぁ。"
