            "voice": "card/Card_13/Card_4130005_1/card_4130005_1_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                13
            ],
            "name": {
                "default": "丸山利恵"
            },
            "message": {
                "default": "これ、どこで手に入れたの？"
