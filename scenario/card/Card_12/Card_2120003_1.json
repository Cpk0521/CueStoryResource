            ],
            "name": {
                "default": "猫"
            },
            "message": {
                "default": "ニャー"
            },
            "voice": ""
        },
        {
            "index": 17,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "ニャーじゃなくてですね！　いろいろこう……、動いてほしいんですよ。\n……ちょっと、枝でつつきますよ。ほれ。"
            },
            "voice": "card/Card_12/Card_2120003_1/card_2120003_1_013.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                0
            ],
            "name": {
