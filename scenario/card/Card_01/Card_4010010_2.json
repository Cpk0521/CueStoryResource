            "voice": "card/Card_01/Card_4010010_2/card_4010010_2_050.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                0
            ],
            "name": {
                "default": "マネージャー"
            },
            "message": {
                "default": "そういうことになるね。　"
